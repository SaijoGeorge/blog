{
  "presets": [
    [
      "env",
      {
        "targets": {
          "browsers": ["last 2 versions"]
        },
        "modules": false
      }
    ],
    "react"
  ],
  "plugins": [
    "transform-class-properties",
    "transform-object-rest-spread",
    "syntax-dynamic-import",
    [
      "styled-components",
      {
        "ssr": true,
        "displayName": false,
        "preprocess": false,
        "minify": true,
        "transpileTemplateLiterals": true
      }
    ]
  ]
}
